{
  "name": "gtew",
  "version": "1.0.0",
  "description": "Global, TypeScript written, Error Handler Wrapper",
  "main": "./dist/gtew.js",
  "private": true,
  "type": "module",
  "types": "./dist/gtew.d.ts",
  "scripts": {
    "build": "npm run build-main && npm run build-test",
    "build-test": "tsc ./tests/backend/error.test.ts --outDir ./tests/backend/ --target esnext --module esnext && tsc ./tests/frontend/error.ts --outDir ./tests/frontend/ --target esnext --module esnext",
    "build-main": "webpack --config ./webpack.config.cjs",
    "run-test": "node ./tests/backend/error.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosunLP/GTEW.git"
  },
  "keywords": [
    "npm",
    "node",
    "browser",
    "error",
    "errorhandler",
    "typescript"
  ],
  "author": "Jonas Pfalzgraf <info@josunlp.de>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/JosunLP/GTEW/issues"
  },
  "homepage": "https://github.com/JosunLP/GTEW#readme",
  "devDependencies": {
    "@types/pouchdb": "^6.4.0",
    "@types/node": "^18.11.18",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "pouchdb": "^8.0.0",
    "simple-toast-messages": "^1.0.2"
  }
}
